# OneTrainerWeb Environment Variables
# Copy this file to .env and modify as needed.

# Backend API server
# BACKEND_PORT=8000
# BACKEND_HOST=127.0.0.1

# Vite dev server
# VITE_PORT=5173

# External backend (skip spawning backend from Electron)
# OT_EXTERNAL_BACKEND=1

# Open Electron DevTools on launch
# OT_DEVTOOLS=1

# Logging
# LOG_LEVEL=INFO

# Disable buffering for real-time logs
# PYTHONUNBUFFERED=1
